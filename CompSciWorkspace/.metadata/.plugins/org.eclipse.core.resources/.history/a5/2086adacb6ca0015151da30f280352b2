public class GenStack <T>{
	private class Node{
		private T data;
		private Node link;
		//default constructor for Node
		public Node() {
			this.data = null;
			this.link = null;
		}
	}
	private Node head, previous, tail;
	
	//creates an empty list
	public GenStack(){
		this.head = null;
		this.tail = null;
	}
	
	public void push(T aData)
	{
		Node newNode = new Node();
		newNode.data = aData;
		if(head == null)
		{
			head = previous = newNode;
			return;
		}
		head = newNode;
		head.link = previous;
		
	}
	
	public T pop()
	{
		if(head!=null){
			head.link = null;
			return head.data;			
		}
		else
		{
			return null;
		}
	}
}
