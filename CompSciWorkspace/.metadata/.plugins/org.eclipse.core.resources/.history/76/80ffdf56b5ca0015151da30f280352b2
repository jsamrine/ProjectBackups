public class GenStack <T>{
	private class Node{
		private T data;
		private Node link;
		//default constructor for Node
		public Node() {
			this.data = null;
			this.link = null;
		}
	}
	private Node head, current, tail;
	
	//creates an empty list
	public GenStack(){
		this.head = null;
		this.current = null;
	}
	
	public void push(T aData)
	{
		Node newNode = new Node();
		newNode.data = aData;
		if(head == null)
		{
			head = tail = newNode;
			return;
		}
		tail.link = newNode;
		tail = tail.link;
		}
	}
	
	public T pop()
	{
		if(current!=null){
			previous.link = null;
			return current.data;			
		}
		else
		{
			return null;
		}
	}
}
