
public class Koenigsburg {

	public static void main(String[] args) {
		Graph g = new Graph(7);
		g.addVertex("V1");
		g.addVertex("V2");
		g.addVertex("V3");
		g.addVertex("V4");
		//g.addVertex("V5");
		g.addEdge("V1", "V2", 1);
		g.addEdge("V1", "V2", 2);
		g.addEdge("V2", "V3", 3);
		g.addEdge("V2", "V3", 4);
		g.addEdge("V2", "V4", 5);
		g.addEdge("V1", "V4", 1);
		g.addEdge("V3", "V4", 1);
		System.out.println("Taking a walk");
		g.strollAroundKoningsburg();
		/*
